from netmiko import ConnectHandler

# Par치metros de conexi칩n al switch Cisco
device = {
    "device_type": "cisco_ios_telnet",
    "host": "20.1.1.1",
    "username": "admin",
    "password": "cisco",
     "port": 23,
    "secret": "cisco"
}

# Establecer conexi칩n Telnet
net_connect = ConnectHandler(**device)

#comandos de configuracion en switch
config_commands = [
    "enable"
    "Ni21Fl35SP1"
    "configure terminal"
    "hostname router"
]

# Ingresar al modo enable
net_connect.enable()

# Comando para cambiar el hostname
hostname_command = "hostname router"

# Enviar comando al dispositivo
output = net_connect.send_config_set(hostname_command)

# Imprimir el resultado del cambio de hostname
print(output)

# Cerrar la conexi칩n
net_connect.disconnect()